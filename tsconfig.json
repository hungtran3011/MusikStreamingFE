{
  "compilerOptions": {
    "target": "ES2017",
    "checkJs": false,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/(auth)/login/page.jsx"
, "app/app-components/navs/nav-rail-common-item.jsx", "app/app-components/navs/bottom-nav-bar-item.jsx", "app/services/auth.service.js", "app/app-components/buttons/filled-button.jsx", "app/(auth)/sign-up/page.jsx", "app/app-components/navs/nav-rail.jsx", "app/app-components/navs/nav-rail-pinned-item.jsx", "app/app-components/buttons/text-button.jsx", "app/app-components/buttons/toggle-button.jsx", "app/app-components/buttons/outlined-button.jsx", "app/app-components/inputs/search-box.jsx", "app/app-components/inputs/date-input.jsx", "app/(main)/layout.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
